{
  "name": "@dtechvision/indexingco-cli",
  "version": "0.0.6",
  "type": "module",
  "packageManager": "bun@1.1.34",
  "license": "MIT",
  "description": "A CLI for indexing.co's Neighbourhood",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtechvision/indexing-co-cli.git"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "tsup && bun run copy-package-json",
    "build:ts": "tsup",
    "clean": "rimraf dist/*",
    "check": "tsc -b tsconfig.json",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "bun run lint --fix",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "copy-package-json": "bun scripts/copy-package-json.ts",
    "changeset-version": "changeset version && bun scripts/version.mjs",
    "changeset-publish": "bun run build && TEST_DIST= bun run test && changeset publish",
    "release": "bun scripts/release.mjs"
  },
  "bun": {
    "patches": {
      "@changesets/assemble-release-plan@6.0.8": "patches/@changesets__assemble-release-plan@6.0.8.patch",
      "@changesets/get-github-info@0.6.0": "patches/@changesets__get-github-info@0.6.0.patch"
    }
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@effect/cli": "latest",
    "@effect/eslint-plugin": "^0.2.0",
    "@effect/language-service": "^0.1.0",
    "@effect/platform": "latest",
    "@effect/platform-node": "latest",
    "@effect/vitest": "^0.27.0",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "effect": "^3.17.7",
    "eslint": "^9.10.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-codegen": "0.28.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "@changesets/get-github-info@0.6.0": "patches/@changesets__get-github-info@0.6.0.patch"
  },
  "bin": {
    "indexingco": "dist/bin.cjs"
  },
  "files": [
    "dist"
  ]
}
